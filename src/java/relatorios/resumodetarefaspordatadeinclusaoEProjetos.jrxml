	<parameter name="idprojeto" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select count(t.*),f.descricao descricaofase,m.descricao descricaomodulo, pri.descricao descricraoprioridade
from tarefa t, fase f, projeto p, MODULO M, prioridade pri
where t.id_fase = f.id
and t.id_projeto = p.id
and t.id_modulo = m.id
and t.id_prioridade = pri.id
and t.datahora_criacao >= $P{datainclusaoinicio}
and t.datahora_criacao <= $P{datahorainclusaofinal}
and t.id_projeto = $P{idprojeto}
group by f.id, p.id, m.id,pri.descricao
order by  count(t.*) desc]]>
	</queryString>
